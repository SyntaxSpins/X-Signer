<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/BG"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="15dp"
		android:layout_marginRight="15dp"
		android:paddingTop="12dp"
		android:paddingBottom="8dp"
		android:orientation="vertical"
		android:background="@drawable/drawable_dialog">
		<LinearLayout
			android:id="@+id/linear2"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginLeft="18dp"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/title"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:paddingLeft="8dp"
				android:paddingTop="8dp"
				android:paddingRight="8dp"
				android:paddingBottom="5dp"
				android:text="Import a new key"
				android:textSize="25sp"
				android:fontFamily="@font/product_sans_regular"
				android:textColor="?colorOnSurface" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear3"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginLeft="18dp"
			android:layout_marginRight="20dp"
			android:paddingTop="10dp"
			android:paddingBottom="25dp"
			android:orientation="vertical">
			<com.google.android.material.textfield.TextInputLayout
				android:id="@+id/KeyStoreTIP"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginRight="8dp"
				android:layout_marginBottom="3dp"
				android:paddingHorizontal="4dp"
				style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
				app:hintEnabled="true"
				app:hintAnimationEnabled="true"
				android:hint="@string/stringKeyStore">
				<com.google.android.material.textfield.TextInputEditText
					android:id="@+id/KeyStoreE"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="16dp"
					android:textSize="16sp"
					android:textColorHint="?colorOutline"
					android:focusable="true"
					android:textColor="@color/md_theme_textMain"
					android:drawableTint="@color/colorAccent" />
			</com.google.android.material.textfield.TextInputLayout>
			<com.google.android.material.textfield.TextInputLayout
				android:id="@+id/AliasTIP"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginRight="8dp"
				android:layout_marginBottom="3dp"
				android:paddingHorizontal="4dp"
				style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
				app:hintEnabled="true"
				app:hintAnimationEnabled="true"
				android:hint="@string/stringAlias">
				<com.google.android.material.textfield.TextInputEditText
					android:id="@+id/AliasE"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="16dp"
					android:textSize="16sp"
					android:textColorHint="#607D8B"
					android:focusable="true"
					android:textColor="@color/md_theme_textMain" />
			</com.google.android.material.textfield.TextInputLayout>
			<com.google.android.material.textfield.TextInputLayout
				android:id="@+id/KeyPassTIP"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginRight="8dp"
				android:paddingHorizontal="4dp"
				style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
				app:hintEnabled="true"
				app:hintAnimationEnabled="true"
				android:hint="@string/stringKeyPass">
				<com.google.android.material.textfield.TextInputEditText
					android:id="@+id/KeyPassE"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="16dp"
					android:textSize="16sp"
					android:textColorHint="#607D8B"
					android:focusable="true"/>
			</com.google.android.material.textfield.TextInputLayout>
			<TextView
				android:id="@+id/note"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="10dp"
				android:padding="8dp"
				android:text="*No need to enter Alias password for pkcs12 keys"
				android:textSize="14sp"
				android:layout_gravity="center_vertical"
				android:fontFamily="@font/product_sans_regular"
				android:textColor="?colorOnSecondaryContainer" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear4"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginLeft="18dp"
			android:layout_marginRight="20dp"
			android:layout_marginBottom="8dp"
			android:paddingLeft="10dp"
			android:paddingTop="10dp"
			android:gravity="right|center_vertical"
			android:orientation="horizontal">
			<LinearLayout
				android:id="@+id/LeftButton"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginRight="3dp"
				android:orientation="vertical"
				android:background="@drawable/legacy_dialog_positive_ripple">
				<TextView
					android:id="@+id/LeftText"
					android:focusable="false"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:paddingLeft="20dp"
					android:paddingTop="13dp"
					android:paddingRight="20dp"
					android:paddingBottom="13dp"
					android:text="Cancel"
					android:textSize="14sp"
					android:textColor="?colorPrimary"
					android:fontFamily="@font/product_sans_regular" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/RightButton"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="2dp"
				android:orientation="vertical"
				android:background="@drawable/legacy_dialog_positive_ripple">
				<TextView
					android:id="@+id/RightText"
					android:focusable="false"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:paddingLeft="20dp"
					android:paddingTop="13dp"
					android:paddingRight="20dp"
					android:paddingBottom="13dp"
					android:text="Import"
					android:textSize="14sp"
					android:textColor="?colorPrimary"
					android:fontFamily="@font/product_sans_regular" />
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
