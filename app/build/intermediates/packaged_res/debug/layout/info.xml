<androidx.coordinatorlayout.widget.CoordinatorLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/_coordinator"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:transitionGroup="true">
	<com.google.android.material.appbar.AppBarLayout
		android:id="@+id/_app_bar"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">
		<com.google.android.material.appbar.CollapsingToolbarLayout
			android:id="@+id/collapsingtoolbar"
			android:layout_width="match_parent"
			android:layout_height="200dp"
			android:fitsSystemWindows="true"
			app:layout_scrollFlags="scroll|exitUntilCollapsed|snap"
			app:titleCollapseMode="scale"
			app:expandedTitleTextAppearance="@style/TextAppearance.M3.Sys.Typescale.HeadlineLarge">
			<com.google.android.material.appbar.MaterialToolbar
				android:id="@+id/_toolbar"
				android:layout_width="match_parent"
				android:layout_height="?attr/actionBarSize"
				android:background="?attr/colorPrimary"
				app:layout_collapseMode="pin"
				app:theme="@style/Theme.Base.Main" />
		</com.google.android.material.appbar.CollapsingToolbarLayout>
	</com.google.android.material.appbar.AppBarLayout>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical"
		app:layout_behavior="@string/appbar_scrolling_view_behavior">
		<androidx.core.widget.NestedScrollView
			android:id="@+id/Scroller"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_weight="1"
			android:background="@color/colorPrimaryDark">
			<LinearLayout
				android:id="@+id/BG"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:paddingLeft="15dp"
				android:paddingTop="35dp"
				android:paddingRight="15dp"
				android:paddingBottom="8dp"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/VersionsLinear"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="vertical"
					android:background="@drawable/legacy_item_ripple">
					<LinearLayout
						android:id="@+id/VersionLinear"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="12dp"
						android:paddingRight="8dp"
						android:paddingBottom="12dp"
						android:orientation="horizontal"
						android:background="@drawable/legacy_top_item_ripple">
						<TextView
							android:id="@+id/VersionNameText"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:text="Version name :"
							android:textSize="18sp"
							android:layout_weight="1"
							android:fontFamily="@font/product_sans_regular"
							android:textColor="@color/colorTextMain" />
						<TextView
							android:id="@+id/VersionNameDetails"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:text="Loading..."
							android:textSize="18sp"
							android:layout_gravity="center_vertical"
							android:fontFamily="@font/product_sans_regular"
							android:textColor="@color/colorTextSecondary" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/Dv1"
						android:layout_width="match_parent"
						android:layout_height="1dp"
						android:layout_marginLeft="15dp"
						android:layout_marginRight="15dp"
						android:orientation="horizontal"
						android:scaleY="0.1"
						android:background="@color/colorTextSecondary" />
					<LinearLayout
						android:id="@+id/VersionCodeLinear"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="12dp"
						android:paddingRight="8dp"
						android:paddingBottom="12dp"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/VersionCodeText"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:text="Version code :"
							android:textSize="18sp"
							android:layout_weight="1"
							android:fontFamily="@font/product_sans_regular"
							android:textColor="@color/colorTextMain" />
						<TextView
							android:id="@+id/VersionCodeDetails"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:text="Loading..."
							android:textSize="18sp"
							android:layout_gravity="center_vertical"
							android:fontFamily="@font/product_sans_regular"
							android:textColor="@color/colorTextSecondary" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/Dv2"
						android:layout_width="match_parent"
						android:layout_height="1dp"
						android:layout_marginLeft="15dp"
						android:layout_marginRight="15dp"
						android:orientation="horizontal"
						android:scaleY="0.1"
						android:background="@color/colorTextSecondary" />
					<LinearLayout
						android:id="@+id/ProductionPhaseLinear"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="12dp"
						android:paddingRight="8dp"
						android:paddingBottom="12dp"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/ProductioPhaseText"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:text="Production phase :"
							android:textSize="18sp"
							android:layout_weight="1"
							android:fontFamily="@font/product_sans_regular"
							android:textColor="@color/colorTextMain" />
						<TextView
							android:id="@+id/ProductionPhaseDetails"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:text="Alpha"
							android:textSize="18sp"
							android:layout_gravity="center_vertical"
							android:fontFamily="@font/product_sans_regular"
							android:textColor="@color/colorTextSecondary" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/UpdateLinear"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="12dp"
						android:paddingRight="8dp"
						android:paddingBottom="12dp"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:transitionName="transition2"
						android:background="@drawable/legacy_bottom_item_ripple">
						<TextView
							android:id="@+id/UpdateText"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:text="Check for updates"
							android:textSize="18sp"
							android:layout_weight="1"
							android:fontFamily="@font/product_sans_regular"
							android:textColor="@color/colorTextMain" />
						<ImageView
							android:id="@+id/UpdateIcon"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:src="@drawable/ic_arrow_right"
							android:scaleType="center" />
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/XEngineVersionLinear"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="25dp"
					android:orientation="vertical"
					android:background="@drawable/legacy_item_ripple">
					<LinearLayout
						android:id="@+id/EngineNameLinear"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="12dp"
						android:paddingRight="8dp"
						android:paddingBottom="12dp"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:background="@drawable/legacy_top_item_ripple">
						<TextView
							android:id="@+id/PoweredByTitle"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:text="Powered By"
							android:textSize="18sp"
							android:layout_gravity="center_horizontal"
							android:layout_weight="1"
							android:fontFamily="@font/product_sans_regular"
							android:textColor="@color/colorTextMain" />
						<com.mursaat.extendedtextview.AnimatedGradientTextView
							android:id="@+id/XEngineLogoAnimated"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:orientation="horizontal"
							android:layout_gravity="center_vertical"
							app:customFont="product_sans.ttf"
							android:textSize="20sp"
							app:colors="@array/logo_colors"
							app:simultaneousColors="2"
							app:angle="0"
							app:speed="1000"
							app:maxFPS="50"
							android:text="X-Engine" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/Dv3"
						android:layout_width="match_parent"
						android:layout_height="1dp"
						android:layout_marginLeft="15dp"
						android:layout_marginRight="15dp"
						android:orientation="horizontal"
						android:scaleY="0.1"
						android:background="@color/colorTextSecondary" />
					<LinearLayout
						android:id="@+id/XEngineContainer"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="12dp"
						android:paddingRight="8dp"
						android:paddingBottom="12dp"
						android:orientation="horizontal"
						android:background="@drawable/legacy_bottom_item_ripple">
						<TextView
							android:id="@+id/XEngineVersionTitle"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:text="Engine Version"
							android:textSize="18sp"
							android:layout_weight="1"
							android:fontFamily="@font/product_sans_regular"
							android:textColor="@color/colorTextMain" />
						<TextView
							android:id="@+id/XEngineVersionDetail"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:padding="8dp"
							android:text="1.0"
							android:textSize="18sp"
							android:layout_gravity="center_vertical"
							android:fontFamily="@font/product_sans_regular"
							android:textColor="@color/colorTextSecondary" />
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/ThanksLinear"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="25dp"
					android:paddingTop="10dp"
					android:orientation="vertical"
					android:background="@drawable/legacy_item_ripple">
					<TextView
						android:id="@+id/SpecialThanksTitle"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="8dp"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="Special thanks to :"
						android:textSize="20sp"
						android:textStyle="bold"
						android:fontFamily="@font/product_sans_regular"
						android:textColor="@color/colorAccent" />
					<LinearLayout
						android:id="@+id/User1"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="3dp"
						android:paddingRight="8dp"
						android:paddingBottom="3dp"
						android:gravity="center_vertical"
						android:orientation="horizontal">
						<de.hdodenhof.circleimageview.CircleImageView
							android:id="@+id/Pic1"
							android:layout_width="50dp"
							android:layout_height="45dp"
							android:layout_marginLeft="6dp"
							android:src="@drawable/ic_tester_1"
							app:civ_border_width="0dp"
							app:civ_border_color="#008DCD"
							app:civ_circle_background_color="#FFFFFF"
							app:civ_border_overlay="true" />
						<LinearLayout
							android:id="@+id/NameAndWhy1"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingRight="8dp"
							android:orientation="vertical">
							<TextView
								android:id="@+id/Username1"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="8dp"
								android:paddingRight="8dp"
								android:gravity="center_vertical"
								android:text="Alex"
								android:textSize="18sp"
								android:fontFamily="@font/product_sans_regular"
								android:textColor="@color/colorTextMain" />
							<TextView
								android:id="@+id/Role1"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="1dp"
								android:paddingRight="8dp"
								android:paddingBottom="8dp"
								android:text="Special tester"
								android:textSize="14sp"
								android:layout_gravity="center_vertical"
								android:fontFamily="@font/product_sans_regular"
								android:textColor="@color/colorTextSecondary" />
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/Dv4"
						android:layout_width="match_parent"
						android:layout_height="1dp"
						android:layout_marginLeft="15dp"
						android:layout_marginRight="15dp"
						android:orientation="horizontal"
						android:scaleY="0.1"
						android:background="@color/colorTextSecondary" />
					<LinearLayout
						android:id="@+id/User2"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="3dp"
						android:paddingRight="8dp"
						android:paddingBottom="3dp"
						android:gravity="center_vertical"
						android:orientation="horizontal">
						<de.hdodenhof.circleimageview.CircleImageView
							android:id="@+id/Pic2"
							android:layout_width="50dp"
							android:layout_height="45dp"
							android:layout_marginLeft="6dp"
							android:src="@drawable/ic_tester_3"
							app:civ_border_width="0dp"
							app:civ_border_color="#008DCD"
							app:civ_circle_background_color="#FFFFFF"
							app:civ_border_overlay="true" />
						<LinearLayout
							android:id="@+id/NameAndWhy2"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingRight="8dp"
							android:orientation="vertical">
							<TextView
								android:id="@+id/Username2"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="8dp"
								android:paddingRight="8dp"
								android:gravity="center_vertical"
								android:text="NEOAPPS"
								android:textSize="18sp"
								android:fontFamily="@font/product_sans_regular"
								android:textColor="@color/colorTextMain" />
							<TextView
								android:id="@+id/Role2"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="1dp"
								android:paddingRight="8dp"
								android:paddingBottom="8dp"
								android:text="Special tester"
								android:textSize="14sp"
								android:layout_gravity="center_vertical"
								android:fontFamily="@font/product_sans_regular"
								android:textColor="@color/colorTextSecondary" />
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/Dv5"
						android:layout_width="match_parent"
						android:layout_height="1dp"
						android:layout_marginLeft="15dp"
						android:layout_marginRight="15dp"
						android:orientation="horizontal"
						android:scaleY="0.1"
						android:background="@color/colorTextSecondary" />
					<LinearLayout
						android:id="@+id/User4"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="3dp"
						android:paddingRight="8dp"
						android:paddingBottom="3dp"
						android:gravity="center_vertical"
						android:orientation="horizontal">
						<de.hdodenhof.circleimageview.CircleImageView
							android:id="@+id/Pic4"
							android:layout_width="50dp"
							android:layout_height="45dp"
							android:layout_marginLeft="6dp"
							android:src="@drawable/ic_tester_4"
							app:civ_border_width="0dp"
							app:civ_border_color="#008DCD"
							app:civ_circle_background_color="#FFFFFF"
							app:civ_border_overlay="true" />
						<LinearLayout
							android:id="@+id/NameAndWhy4"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingRight="8dp"
							android:orientation="vertical">
							<TextView
								android:id="@+id/Username4"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="8dp"
								android:paddingRight="8dp"
								android:gravity="center_vertical"
								android:text="Aquiles Trindade"
								android:textSize="18sp"
								android:fontFamily="@font/product_sans_regular"
								android:textColor="@color/colorTextMain" />
							<TextView
								android:id="@+id/Role4"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="1dp"
								android:paddingRight="8dp"
								android:paddingBottom="8dp"
								android:text="Special tester"
								android:textSize="14sp"
								android:layout_gravity="center_vertical"
								android:fontFamily="@font/product_sans_regular"
								android:textColor="@color/colorTextSecondary" />
						</LinearLayout>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/Dv6"
						android:layout_width="match_parent"
						android:layout_height="1dp"
						android:layout_marginLeft="15dp"
						android:layout_marginRight="15dp"
						android:orientation="horizontal"
						android:scaleY="0.1"
						android:background="@color/colorTextSecondary" />
					<LinearLayout
						android:id="@+id/User3"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="3dp"
						android:paddingRight="8dp"
						android:paddingBottom="5dp"
						android:gravity="center_vertical"
						android:orientation="horizontal"
						android:background="@drawable/legacy_bottom_item_ripple">
						<de.hdodenhof.circleimageview.CircleImageView
							android:id="@+id/Pic3"
							android:layout_width="50dp"
							android:layout_height="45dp"
							android:layout_marginLeft="6dp"
							android:src="@drawable/ic_tester_2"
							app:civ_border_width="0dp"
							app:civ_border_color="#008DCD"
							app:civ_circle_background_color="#FFFFFF"
							app:civ_border_overlay="true" />
						<LinearLayout
							android:id="@+id/NameAndWhy3"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingRight="8dp"
							android:orientation="vertical">
							<TextView
								android:id="@+id/Username3"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="8dp"
								android:paddingRight="8dp"
								android:gravity="center_vertical"
								android:text="Prakhar Doneria"
								android:textSize="18sp"
								android:fontFamily="@font/product_sans_regular"
								android:textColor="@color/colorTextMain" />
							<TextView
								android:id="@+id/Role3"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="1dp"
								android:paddingRight="8dp"
								android:paddingBottom="8dp"
								android:text="Special tester"
								android:textSize="14sp"
								android:layout_gravity="center_vertical"
								android:fontFamily="@font/product_sans_regular"
								android:textColor="@color/colorTextSecondary" />
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
		</androidx.core.widget.NestedScrollView>
	</LinearLayout>
</androidx.coordinatorlayout.widget.CoordinatorLayout>
