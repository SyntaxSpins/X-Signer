<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/KeyInfoLayout"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="8dp"
		android:layout_marginTop="2dp"
		android:layout_marginRight="8dp"
		android:layout_marginBottom="2dp"
		android:orientation="vertical"
		android:background="@drawable/legacy_item_ripple">
		<LinearLayout
			android:id="@+id/TopLayout"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:paddingLeft="8dp"
			android:paddingTop="12dp"
			android:paddingRight="8dp"
			android:paddingBottom="5dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<LinearLayout
				android:id="@+id/KeyFrame"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:padding="5dp"
                android:background="@drawable/ic_container_main"
				android:orientation="vertical">
				<ImageView
					android:id="@+id/KeyIcon"
					android:focusable="false"
					android:layout_width="24dp"
					android:layout_height="24dp"
                    android:tint="?colorOnPrimaryContainer"
					android:src="@drawable/ic_vpn_key_black"
					android:scaleType="fitCenter" />
			</LinearLayout>
			<TextView
				android:id="@+id/KeyName"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="Loading..."
				android:textSize="18sp"
				android:fontFamily="@font/product_sans_regular"
				android:textColor="?colorOnSecondaryContainer" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/TypeLinear"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginBottom="10dp"
			android:paddingLeft="8dp"
			android:paddingRight="8dp"
			android:paddingBottom="3dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/TypeTitle"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:paddingLeft="8dp"
				android:paddingTop="5dp"
				android:paddingRight="8dp"
				android:paddingBottom="8dp"
				android:gravity="center_vertical"
				android:text="Key type :"
				android:textSize="14sp"
				android:fontFamily="@font/product_sans_regular"
				android:textColor="?colorOnSecondaryContainer" />
			<TextView
				android:id="@+id/TypeDesc"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:paddingLeft="8dp"
				android:paddingTop="5dp"
				android:paddingRight="8dp"
				android:paddingBottom="8dp"
				android:text="Loading"
				android:textSize="14sp"
				android:layout_gravity="center_vertical"
				android:fontFamily="@font/product_sans_regular"
				android:textColor="?colorOnSurface" />
		</LinearLayout>
		<com.kyo.expandablelayout.ExpandableLayout
			android:id="@+id/FixContainer"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/DataToExpandLinear"
				android:layout_width="match_parent"
                app:canExpand="true"
                app:startExpanded="false"
				android:layout_height="wrap_content"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/Dv1"
					android:layout_width="match_parent"
					android:layout_height="1dp"
					android:layout_marginLeft="15dp"
					android:layout_marginRight="15dp"
					android:orientation="horizontal"
					android:scaleY="0.1"
					android:background="@color/colorTextSecondary" />
				<LinearLayout
					android:id="@+id/AliasLinear"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="3dp"
					android:paddingRight="8dp"
					android:paddingBottom="3dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/AliasTitle"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="Key Alias :"
						android:textSize="14sp"
						android:fontFamily="@font/product_sans_regular"
						android:textColor="?colorOnSecondaryContainer" />
					<TextView
						android:id="@+id/AliasDesc"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Loading"
						android:textSize="14sp"
						android:layout_gravity="center_vertical"
						android:fontFamily="@font/product_sans_regular"
						android:textColor="?colorOnSurface" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/Sha1Linear"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="3dp"
					android:paddingRight="8dp"
					android:paddingBottom="3dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/Sha1Title"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="SHA-1 digest :"
						android:textSize="14sp"
						android:fontFamily="@font/product_sans_regular"
						android:textColor="?colorOnSecondaryContainer" />
					<TextView
						android:id="@+id/Sha1Desc"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Loading"
						android:textSize="14sp"
						android:layout_gravity="center_vertical"
						android:fontFamily="@font/product_sans_regular"
						android:textColor="?colorOnSurface" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/Sha256Linear"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="3dp"
					android:paddingRight="8dp"
					android:paddingBottom="3dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/Sha256Title"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="SHA-256 digest :"
						android:textSize="14sp"
						android:fontFamily="@font/product_sans_regular"
						android:textColor="?colorOnSecondaryContainer" />
					<TextView
						android:id="@+id/Sha256Desc"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Loading"
						android:textSize="14sp"
                        android:gravity="center_vertical"
						android:layout_gravity="center_vertical"
						android:fontFamily="@font/product_sans_regular"
						android:textColor="?colorOnSurface" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/Sha512Linear"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="3dp"
					android:paddingRight="8dp"
					android:paddingBottom="3dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/Sha512Title"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="SHA-512 digest :"
						android:textSize="14sp"
						android:fontFamily="@font/product_sans_regular"
						android:textColor="?colorOnSecondaryContainer" />
					<TextView
						android:id="@+id/Sha512Desc"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Loading"
						android:textSize="14sp"
						android:layout_gravity="center_vertical"
						android:fontFamily="@font/product_sans_regular"
						android:textColor="?colorOnSurface" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/ExpiryLinear"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:paddingLeft="8dp"
					android:paddingTop="3dp"
					android:paddingRight="8dp"
					android:paddingBottom="3dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/KeyExpiryTitle"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="Expiry Date :"
						android:textSize="14sp"
						android:fontFamily="@font/product_sans_regular"
						android:textColor="?colorOnSecondaryContainer" />
					<TextView
						android:id="@+id/KeyExpiryDesc"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Loading"
						android:textSize="14sp"
						android:layout_gravity="center_vertical"
						android:fontFamily="@font/product_sans_regular"
						android:textColor="?colorOnSurface" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear2"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginBottom="10dp"
					android:gravity="center_vertical"
					android:orientation="horizontal"
					android:alpha="0.0">
					<TextView
						android:id="@+id/textview3"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="3dp"
						android:gravity="center_vertical"
						android:text="Expiry Date :"
						android:textSize="14sp"
						android:fontFamily="@font/product_sans_regular"
						android:textColor="?colorOnSecondaryContainer" />
					<TextView
						android:id="@+id/textview4"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="3dp"
						android:text="Loading"
						android:textSize="14sp"
						android:layout_gravity="center_vertical"
						android:fontFamily="@font/product_sans_regular"
						android:textColor="?colorOnSurface" />
				</LinearLayout>
			</LinearLayout>
		</com.kyo.expandablelayout.ExpandableLayout>
	</LinearLayout>
</LinearLayout>
