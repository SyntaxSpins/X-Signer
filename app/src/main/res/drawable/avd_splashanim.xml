<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="20dp"
            android:height="20dp"
            android:viewportWidth="20"
            android:viewportHeight="20"
            android:alpha="0">
            <group
                android:name="group"
                android:translateX="-2"
                android:translateY="-2"
                android:scaleX="0"
                android:scaleY="0">
                <path
                    android:name="path"
                    android:pathData="M 16.56 14.64 C 16.85 8.82 7.39 8.82 7.44 14.64 L 16.56 14.64 Z M 9.6 11.99 C 10.01 11.99 10.33 12.31 10.33 12.72 C 10.33 13.13 10.01 13.45 9.6 13.45 C 9.19 13.45 8.87 13.13 8.87 12.72 C 8.87 12.31 9.19 11.99 9.6 11.99 Z M 14.16 11.99 C 14.57 11.99 14.89 12.31 14.89 12.72 C 14.89 13.13 14.57 13.45 14.16 13.45 C 13.75 13.45 13.43 13.13 13.43 12.72 C 13.43 12.31 13.75 11.99 14.16 11.99 Z"
                    android:fillColor="@color/colorTextMain"
                    android:strokeColor="@color/colorTextMain"
                    android:strokeWidth="0.6"
                    android:strokeLineCap="square"
                    android:strokeLineJoin="round"/>
                <path
                    android:name="path_1"
                    android:pathData="M 9.43 10.87 L 8.62 9.36"
                    android:fillColor="#00FFFFFF"
                    android:strokeColor="@color/colorTextMain"
                    android:strokeWidth="0.48"
                    android:strokeLineCap="round"
                    android:strokeLineJoin="round"/>
                <path
                    android:name="path_2"
                    android:pathData="M 14.88 10.88 L 15.69 9.36"
                    android:fillColor="#00FFFFFF"
                    android:strokeColor="@color/colorTextMain"
                    android:strokeWidth="0.48"
                    android:strokeLineCap="round"
                    android:strokeLineJoin="round"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:startOffset="100"
                    android:duration="636"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="100"
                    android:duration="636"
                    android:valueFrom="0.1"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:startOffset="100"
                    android:duration="636"
                    android:valueFrom="10"
                    android:valueTo="-2"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:startOffset="100"
                    android:duration="636"
                    android:valueFrom="23.5"
                    android:valueTo="-2"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="vector">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="alpha"
                android:startOffset="100"
                android:duration="150"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/linear_out_slow_in"/>
        </aapt:attr>
    </target>
</animated-vector>
